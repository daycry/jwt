# JWT Testing Environment Configuration
# This file is used during PHPUnit tests

#--------------------------------------------------------------------
# Environment
#--------------------------------------------------------------------
app.environment = testing

#--------------------------------------------------------------------
# JWT Settings for Testing
#--------------------------------------------------------------------

# Test signing key will be added by command tests when needed
# jwt.signer=

# Test issuer
jwt.issuer=https://test.example.com

# Test audience
jwt.audience=https://test.example.com

# Test identifier
jwt.identifier=jwt-test-app

# Short expiration for testing
jwt.canOnlyBeUsedAfter="+0 minute"
jwt.expiresAt="+1 hour"

# Test algorithm
jwt.algorithm="Lcobucci\JWT\Signer\Hmac\Sha256"

# Enable exceptions for better testing
jwt.throwable=true

# Enable validation
jwt.validate=true

#--------------------------------------------------------------------
# Test Database Settings (if needed)
#--------------------------------------------------------------------
database.default.hostname = localhost
database.default.database = jwt_test
database.default.username = root
database.default.password = 
database.default.DBDriver = MySQLi

